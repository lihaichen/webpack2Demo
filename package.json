{
  "name": "webpack2",
  "version": "1.0.0",
  "description": "webpack2",
  "main": "./src/index.js",
  "scripts": {
    "test": "better-npm-run gen-dev-config && better-npm-run build",
    "dev": "better-npm-run gen-dev-config &&  better-npm-run dev",
    "build": "better-npm-run gen-dev-config && better-npm-run build",
    "prod": "better-npm-run gen-dev-config && better-npm-run prod"
  },
  "betterScripts": {
    "gen-dev-config": {
      "command": "node ./bin/gen_config.js",
      "env": {
        "NODE_ENV": "dev"
      }
    },
    "gen-build-config": {
      "command": "node ./bin/gen_config.js",
      "env": {
        "NODE_ENV": "build"
      }
    },
    "gen-prod-config": {
      "command": "node ./bin/gen_config.js",
      "env": {
        "NODE_ENV": "prod"
      }
    },
    "dev": {
      "command": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config ./webpack/dev.config.js",
      "env": {
        "NODE_ENV": "dev"
      }
    },
    "build": {
      "command": "node_modules/webpack/bin/webpack.js --config ./webpack/dev.config.js",
      "env": {
        "NODE_ENV": "build"
      }
    },
    "prod": {
      "command": "node_modules/webpack/bin/webpack.js --config ./webpack/dev.config.js",
      "env": {
        "NODE_ENV": "build"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lihaichen/webpack2"
  },
  "author": "lihaichen",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-display-name": "^6.3.13",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-plugin-typecheck": "^3.6.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "better-npm-run": "0.0.15",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.1",
    "eslint": "1.10.3",
    "eslint-config-airbnb": "0.1.0",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-import": "^0.8.0",
    "eslint-plugin-react": "^3.5.0",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "install": "^0.10.0",
    "json-loader": "^0.5.4",
    "less": "^2.7.2",
    "less-loader": "^4.0.3",
    "npm": "^4.5.0",
    "post-loader": "^1.1.2",
    "precss": "^1.4.0",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.3.2",
    "style-loader": "^0.13.0",
